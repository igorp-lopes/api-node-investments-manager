{
  "name": "api-node-investments-manager",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "yarn pre:dev && dotenv -e .env.dev nest start",
    "start:dev": "yarn pre:dev && dotenv -e .env.dev nest start --watch",
    "start:debug": "yarn pre:dev && dotenv -e .env.dev nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "applyDevMigrations": "dotenv -e .env.dev yarn prisma migrate dev",
    "prismaDevDbPush": "dotenv -e .env.dev yarn prisma db push",
    "db:test:start": "docker-compose --env-file .env.test up -d",
    "db:test:stop": "docker-compose --env-file .env.test down -v",
    "db:dev:start": "docker-compose --env-file .env.dev up -d",
    "db:dev:stop": "docker-compose --env-file .env.dev down -v",
    "pre:test": "yarn db:test:start && dotenv -e .env.test yarn prisma migrate deploy",
    "post:test": "yarn db:test:stop",
    "pre:dev": "yarn db:dev:stop && yarn db:dev:start && sleep 1 && dotenv -e .env.dev yarn prisma migrate deploy",
    "post:dev": "yarn db:dev:stop",
    "test": "dotenv -e .env.test jest --config ./jest-spec.json",
    "test:watch": "dotenv -e .env.test -- jest --watch --config ./jest-spec.json",
    "test:cov": "dotenv -e .env.test -- jest --coverage --config ./jest-spec.json",
    "test:debug": "dotenv -e .env.test -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "yarn pre:test && dotenv -e .env.test -- jest --config ./jest-e2e.json && yarn post:test"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/platform-express": "^8.4.0",
    "@prisma/client": "^3.14.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv-cli": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.2",
    "@nestjs/schematics": "^8.0.7",
    "@nestjs/testing": "^8.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.3.2",
    "prisma": "^3.14.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.7",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.13.0",
    "typescript": "^4.6.2"
  }
}
